#page-content-wrapper
  .container-fluid
    .header
      =render "shared/titlebar", :title => t('appointment.panel.title'),:text => t(".app_name")
    %div.container
      %div.row
        = render partial: "shared/messages", flash: flash
      %div.jumbotron
        %h3.text-center
          = t(".learning_center")
        %h4.text-center
          = t(".admin_resources")
        %div.row
          %div.col-md-4.col-md-offset-4
            %div.list-group.text-center
              = link_to t("button.go"), admins_resources_path, class: "list-group-item"
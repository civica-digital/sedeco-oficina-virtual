<div id=<%="#{locals[:number]}"%>>
	<p> 
		<%= locals[:texto_question] %> 
	</p>
	<BR>
  <div class="col-md-12">
    <div class="col-md-6 col-md-offset-3">
      <div class="row">
        <%locals[:texto_answer].each_with_index do |text, index|%>
          <div class=<%="col-md-#{12/locals[:texto_answer].size}"%>>
            <%=image_tag locals[:imagen][index], class: "img-responsive"%>
          </div>
        <%end%>
      </div>
    </div>
  </div>
</div>

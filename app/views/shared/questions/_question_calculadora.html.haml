          %i.fa.fa-info-circle.input-gray{:href => "#menu-toggle",:id=>"menu-toggle92"}
          =text_field :date, "date", class: 'form-controler',:type=>"number", :placeholder => '45.5',
          :onchange=> "valueAforo('',#{session[:size_business].to_f})",:id=>"mobiliario_t"
      %hr.style-line
      %div.row  
        .col-md-12
          Giro
          %i.fa.fa-info-circle.input-gray{:href => "#menu-toggle",:id=>"menu-toggle93"}
          -if is_bajo_impacto?
            =select :date, "date", 
            Towns.load_aforo('bajo').each_with_index.collect {|opt| [opt[:label], opt[:id], opt[:name]]},{ include_blank: true , selected: false },:id=>"impacto_t", class: "chosen-select",:onchange=> "valueAforo('bajo',#{session[:size_business].to_f})"
          -else
            =select :date, "date", 
            Towns.load_aforo('vecinal').each_with_index.collect {|opt| [opt[:label], opt[:id], opt[:name]]},{ include_blank: true , selected: false },:id=>"impacto_t", class: "chosen-select",:onchange=> "valueAforo('vecinal',#{session[:size_business].to_f})"


  

%div.margin-top-dropdown.hidden{:id=>"diagnosticaforo"} 
  .jumbotron
    %h2.colorprimario
      ="Tu aforo es: #@aforo"

:javascript
  $(document).ready( function () {
    for(i = 90; i <= 97; i++){
      $("#menu-toggle"+i).click(function(e) {
        e.preventDefault();
        $('#head_wrapper').css("position", "fixed");
        $('#head_wrapper').css("width", "50%");
        $('#menu-toggleint').css("position", "fixed");
        $("#wrapper").toggleClass("toggled");
        
        if($("#wrapper").hasClass("toggled")){
          $('#page-content-wrapper').css("-webkit-filter", "blur(0px)");
          $('#page-content-wrapper').css("-webkit-transition", "-webkit-filter .0s");
        }else{
          $('#page-content-wrapper').css("-webkit-filter", "blur(5px)");
          $('#page-content-wrapper').css("-webkit-transition", "-webkit-filter .30s");
        }
      });
    }
  });
          =text_field :date, "date", class: 'form-controler',:type=>"number", :placeholder => '100.5',
          :onchange=> "valueAforo('',#{session[:size_business].to_f})",:id=>"superficie_t"
        .col-md-2
          .col-md-10.col-md-offset-1

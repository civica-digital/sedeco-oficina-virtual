%div.margin-top-dropdown
  %div.row
    .col-md-5
      Superficie del establecimiento
      =select :date, "date", 
      get_array_values(1,'rango').each_with_index.map {|name, index| [name,index]},{ include_blank: true , selected: false }, class: "chosen-select"
    .col-md-2
      .col-md-6.col-md-offset-3
        %hr.style-line
    .col-md-5
      Superficie del mobiliario
      =select :date, "date", 
      get_array_values(1,'rango').each_with_index.map {|name, index| [name,index]},{ include_blank: true , selected: false }, class: "chosen-select"
  %hr.style-line
  %div.row  
    .col-md-6.col-md-offset-3
      superficie del giro
      -if bajo_impacto?
        =select :date, "date", 
        Services.load_aforo('bajo').each_with_index.collect {|opt| [opt[:label], opt[:id]]},{ include_blank: true , selected: false }, class: "chosen-select"
      -else
        =select :date, "date", 
        Services.load_aforo('vecinal').each_with_index.collect {|opt| [opt[:label], opt[:id]]},{ include_blank: true , selected: false }, class: "chosen-select"
